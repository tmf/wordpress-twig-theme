<!-- Footer Wrapper -->
<div id="footer-wrapper" class="wrapper">
    <div class="title">The Rest Of It</div>
    <div class="container">
        <div class="row">
            <div class="12u">

                <!-- Footer -->
                <div id="footer">
                    {% block footer %}
                    <header class="style1">
